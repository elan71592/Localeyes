<div class='container'>
  <div class="row">
    <h1><%=@tag.name%></h1>
  </div>

  <div class="row">
      <section id="trip-list-holder">
        <% @trips.each do |trip| %>
        <div class="col s12 m6 l6">
          <%= render partial: 'trips/trip_card', locals: { trip: trip } %>
        </div>
        <% end %>
      </section>
  </div>
</div>
